public class pOf1Assignment4 {
    public static void main(String[] args){
        int a=10, b=2;
        int sum=a+b;
        int multi=a*b;
        int sub=a-b;
        double div=a/b;

        System.out.println("Sum of " +a + " and " +b +" is: " +sum);
        System.out.println("Product of " +a +" and " +b +" is: " +multi);
        System.out.println("Subtraction of " +a +" from " +b +" is: " +sub);
        System.out.println("Division between " +a +" and " +b +" is: " +div);


    }
}
